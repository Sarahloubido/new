{
  "name": "prototype-text-review-tool",
  "version": "1.0.0",
  "description": "Extract text from design prototypes, edit in spreadsheets, and reapply changes",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "npx react-scripts start",
    "build": "npx react-scripts build",
    "build:alt": "node build.js",
    "start": "node server/index.js",
    "setup": "npm install",
    "test": "npx react-scripts test",
    "eject": "npx react-scripts eject"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "axios": "^1.6.2",
    "figma-api": "^1.11.0",
    "papaparse": "^5.4.1",
    "xlsx": "^0.18.5",
    "sharp": "^0.33.1",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "body-parser": "^1.20.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "figma",
    "prototype",
    "text-review",
    "design-tools",
    "spreadsheet"
  ],
  "author": "Cursor AI",
  "license": "MIT"
}